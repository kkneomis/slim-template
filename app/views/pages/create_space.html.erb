<div class="page-header full-content">
  <div class="row">
    <div class="col-sm-6">
      <h1>ADD A SPACE </h1>
    </div><!--.col-->
    <div class="col-sm-6">
      <ol class="breadcrumb">
        <li><a href="#"><i class="ion-home"></i></a></li>
        <li><a href="#">Forms</a></li>
        <li><a href="#" class="active">Pickers</a></li>
      </ol>
    </div><!--.col-->
  </div><!--.row-->
</div><!--.page-header-->

<div class="row">
  <div class="col-md-12">
    <div class="panel">
      <div class="panel-heading">
        <div class="panel-title"><h4>CREATING YOUR SPACE...</h4></div>
      </div><!--.panel-heading-->
      <div class="panel-body">
        <form action="#" class="form-horizontal">
          <div class="form-content">
            <div class="form-group">
              <label class="control-label col-md-3">Space Name</label>
              <div class="col-md-5">
                <div class="inputer">
                  <div class="input-wrapper">
                    <input type="text" name="" class="form-control" autofocus>
                  </div>
                </div>
              </div>
            </div><!--.form-group-->

            <div class="form-group">
              <label class="control-label col-md-3">Capacity</label>
              <div class="col-md-5">
                <div class="inputer">
                  <div class="input-wrapper">
                    <input type="text" name="" class="form-control" placeholder="This space seats....people">
                  </div>
                </div>
              </div>
            </div><!--.form-group-->

            <div class="form-group">
              <label class="control-label col-md-3">Description</label>
              <div class="col-md-5">
                <div class="inputer">
                  <div class="input-wrapper">
                    <textarea name="" class="form-control"></textarea>
                  </div>
                </div>
              </div>
            </div><!--.form-group-->


            <div class="form-group">
              <label class="control-label col-md-3">Add an Image</label>
              <div class="col-md-9">
                <div class="form-group">
                  <input type="file" id="spaceimage" class="btn btn-default">
                </div><!--.form-group-->
              </div><!--.col-md-9-->
            </div><!--.row-->

            <div class="form-buttons">
              <div class="row">
                <div class="col-md-offset-3 col-md-9">
                  <button type="submit" class="btn btn-blue">Submit</button>
                  <button type="button" class="btn btn-flat btn-default">Cancel</button>
                </div>
              </div>
            </div>
          </div>
        </form>
      </div><!--.panel-body-->
    </div><!--.panel-->
  </div><!--.col-md-12-->
</div><!--.row-->

